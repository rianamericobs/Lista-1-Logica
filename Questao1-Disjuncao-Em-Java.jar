public class Main {
    public static void main(String[] args) {
        int a = 5;
        int b = 10;
        int c = 3;

        // Exemplo 1: Disjunção em uma decisão
        if (a > b || c < b) {
            System.out.println("a é maior que b ou c é menor que b"); // Verdadeiro, pois (5 > 10) || (3 < 10)
        }

        // Exemplo 2: Disjunção em uma atribuição
        boolean result = (a > b) || (c > b);
        System.out.println("Resultado: " + result); // Falso, pois (5 > 10) || (3 > 10) é falso (result = false)
    }
}
