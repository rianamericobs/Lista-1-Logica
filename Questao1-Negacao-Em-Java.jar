public class Main {
    public static void main(String[] args) {
        int a = 5;
        int b = 10;

        // Exemplo 1: Negação em uma decisão
        if (!(a > b)) {
            System.out.println("a não é maior que b"); // Verdadeiro, pois !(5 > 10) é verdadeiro
        }

        // Exemplo 2: Negação em uma atribuição
        boolean result = !(a < b);
        System.out.println("Resultado: " + result); // Falso, pois !(5 < 10) é falso (result = false)
    }
}
